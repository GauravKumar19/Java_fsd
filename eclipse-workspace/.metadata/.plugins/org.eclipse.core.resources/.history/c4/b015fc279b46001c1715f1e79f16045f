package newfeatures;

public interface vehicle {

	//By Default all are abstract methods
	String getBrand();
	String speedUp();
	String slowDown();
	
	default String turnAlarmOn()
	{
	return "Turning the Vehicle Alarm ON.";
	}
	
	default String turnAlarmoff()
	{
	return "Turning the Vehicle Alarm OFF.";
	}
	
}
