package newfeatures;

// perform arithmetic operation using lambda
public class lambdademo4 {
	public static void main(String[] args) {
	
		addition op1=(a,b) -> (a+b);
		System.out.println("addition of two number is :" op1.calculate(200,155));
		
		addition op2=(a,b) -> (a-b);
		System.out.println("Subtraction of two number is :" op1.calculate(200,155));

		
		addition op3=(a,b) -> (a*b);
		System.out.println("Multiplication of two number is :" op1.calculate(200,155));

		
		addition op4=(a,b) -> (a/b);
		System.out.println("Division of two number is :" op1.calculate(200,155));

		
	}
	
}
